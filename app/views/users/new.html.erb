<h1> WELCOME TO WIKI PAGE</h1>
<%= form_with model :user do |form|%>
<%form.label :username, "Username: "%>
<%= form.text_field :username, placeholder="Enter your username"%><br/>
<%= form.label  :email, "Email"%>
<%= form.text_field :email , placeholder="Enter your email address"%><br/>
<%= form.label  :password, "Password"%>
<%= form.password_field :password , placeholder="enter your password"%>
<%= form.submit "Submit"%>

<% end %>

